{
    "name": "theadeyemiolayinka/test-fb-lara-auth",
    "description": "A Laravel Package to utilize firebase as default authentication method.",
    "type": "library",
    "homepage": "https://github.com/theadeyemiolayinka/fb-lara-auth",
    "support": {
        "issues": "https://github.com/theadeyemiolayinka/fb-lara-auth",
        "source": "https://github.com/theadeyemiolayinka/fb-lara-auth"
    },
    "autoload": {
        "psr-4": {
            "TheAdeyemiOlayinka\\FbLaraAuth\\": "src/"
        },
        "files": [
            "src/Helpers/Helper.php"
        ]
    },
    "authors": [
        {
            "name": "Olayinka Adeyemi",
            "homepage": "https://github.com/theadeyemiolayinka",
            "email": "olayinkaapps@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "TheAdeyemiOlayinka\\FbLaraAuth\\FbLaraAuthServiceProvider",
                "Kreait\\Laravel\\Firebase\\ServiceProvider"
            ]
        }
    },
    "require": {
        "kreait/laravel-firebase": "^4.2",
        "php": ">=7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^8.4|^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
